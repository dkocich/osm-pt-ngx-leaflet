{
  "name": "osm-pt-ngx-leaflet",
  "version": "0.0.1",
  "engines": {
    "node": "7.2.1"
  },
  "description": "An OSM public transport editor based on Leaflet and Angular JS framework.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dkocich/osm-pt-ngx-leaflet.git"
  },
  "keywords": [
    "angular",
    "leaflet",
    "starter"
  ],
  "author": "David Kocich <dkocich@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dkocich/osm-pt-ngx-leaflet/issues"
  },
  "scripts": {
    "build": "webpack --config webpack/webpack.config.prod.js --colors",
    "build-dev": "webpack --config webpack/webpack.config.dev.js --progress --colors",
    "deploy": "NODE_ENV=production webpack -p --config webpack/webpack.config.prod.js --colors",
    "dev": "webpack-dev-server --config webpack/webpack.config.dev.js  --content-base public/ --progress --colors",
    "postinstall": "NODE_ENV=production webpack -p --config webpack/webpack.config.dev.js --progress --colors",
    "start": "webpack-dev-server --content-base public/",
    "test": "snyk test && NODE_ENV=development karma start",
    "watch": "webpack --config webpack/webpack.config.dev.js --watch",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "homepage": "https://github.com/haoliangyu/ngx-leaflet-starter#readme",
  "dependencies": {
    "@angular/common": "^4.1.3",
    "@angular/compiler": "^4.1.3",
    "@angular/core": "^4.1.3",
    "@angular/forms": "^4.1.3",
    "@angular/http": "^4.1.3",
    "@angular/platform-browser": "^4.1.3",
    "@angular/platform-browser-dynamic": "^4.1.3",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-alpha.26",
    "@types/es6-shim": "^0.31.34",
    "@types/geojson": "1.0.2",
    "@types/leaflet": "^1.0.62",
    "bootstrap": "^3.3.7",
    "express": "^4.15.3",
    "font-awesome": "^4.7.0",
    "leaflet": "^1.0.3",
    "leaflet.vectorgrid": "^1.2.0",
    "ngx-bootstrap": "^1.7.1",
    "osmtogeojson": "^3.0.0-beta.2",
    "reflect-metadata": "^0.1.10",
    "rxjs": "5.4.0",
    "snyk": "^1.33.0",
    "zone.js": "^0.8.11"
  },
  "devDependencies": {
    "@angular/cli": "^1.1.0",
    "@angular/compiler-cli": "^4.1.3",
    "@types/jasmine": "^2.5.51",
    "@types/node": "^7.0.27",
    "concurrently": "^3.4.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.2",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "jasmine": "^2.6.0",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "less": "^2.7.2",
    "less-loader": "^4.0.4",
    "optimize-css-assets-webpack-plugin": "^1.3.2",
    "optimize-js-plugin": "0.0.4",
    "script-ext-html-webpack-plugin": "^1.8.1",
    "style-loader": "^0.18.2",
    "to-string-loader": "^1.1.5",
    "ts-loader": "^2.1.0",
    "tslint": "^5.4.2",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.3.4",
    "url-loader": "^0.5.8",
    "webpack": "^2.6.1",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-dev-server": "^2.4.5"
  },
  "snyk": true
}
