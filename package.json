  },
  "scripts": {
    "cypress-open": "node_modules/.bin/cypress open",
    "cypress-run": "node_modules/.bin/cypress run --record --key 5bed002a-82c7-44a6-9e99-8fee9727f7d2",
    "lint": "tslint -c tslint.json public_src/**/*.ts public_src/components/**/*.ts public_src/*.ts",
    "postinstall": "npm run ngbuild",
    "prepare": "npm run snyk-protect",
    "prepublishOnly": "snyk protect",
    "snyk-protect": "snyk protect",
    "stylelint": "stylelint **/*.css **/*.less",
    "test": "snyk test && NODE_ENV=development karma start",
    "ng": "ng",
    "ngstart": "ng serve",
    "ngstart-hmr": "ng serve --configuration hmr --hmr --aot=true",
    "ngstart-aot": "ng serve --aot=true",
    "ngbuild": "ng build --progress --verbose --prod --aot=true",
    "ngtest": "ng test",
    "nglint": "ng lint",
    "nge2e": "ng e2e"
