{
  "name": "osm-pt-ngx-leaflet",
  "version": "0.0.1",
  "engines": {
    "node": "8.1.4"
  },
  "description": "An OSM public transport editor based on Leaflet and Angular JS framework.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dkocich/osm-pt-ngx-leaflet.git"
  },
  "keywords": [
    "angular",
    "leaflet",
    "starter"
  ],
  "author": "David Kocich <dkocich@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dkocich/osm-pt-ngx-leaflet/issues"
  },
  "scripts": {
    "build": "webpack --config webpack/webpack.config.prod.js --colors",
    "build-dev": "webpack --config webpack/webpack.config.dev.js --progress --colors",
    "deploy": "NODE_ENV=production webpack -p --config webpack/webpack.config.prod.js --colors",
    "dev": "NODE_ENV=development webpack-dev-server --config webpack/webpack.config.dev.js  --content-base public/ --progress --colors",
    "lint": "tslint -c tslint.json public_src/**/*.ts",
    "postinstall": "NODE_ENV=production webpack -p --config webpack/webpack.config.dev.js --progress --colors",
    "start": "webpack-dev-server --content-base public/",
    "test": "snyk test && NODE_ENV=development karma start",
    "watch": "webpack --config webpack/webpack.config.dev.js --watch",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "homepage": "https://github.com/haoliangyu/ngx-leaflet-starter#readme",
  "dependencies": {
    "@angular/animations": "4.3.2",
    "@angular/common": "4.3.2",
    "@angular/compiler": "4.3.2",
    "@angular/core": "4.3.2",
    "@angular/forms": "4.3.2",
    "@angular/http": "4.3.2",
    "@angular/platform-browser": "4.3.2",
    "@angular/platform-browser-dynamic": "4.3.2",
    "@types/es6-shim": "0.31.34",
    "@types/geojson": "1.0.2",
    "@types/leaflet": "1.0.67",
    "@types/xmlbuilder": "0.0.30",
    "angular2-busy": "2.0.4",
    "angular2-toaster": "4.0.1",
    "bootstrap": "3.3.7",
    "cssnano": "^3.10.0",
    "express": "4.15.3",
    "font-awesome": "4.7.0",
    "leaflet": "1.1.0",
    "leaflet.vectorgrid": "1.2.0",
    "ng2-dragula": "1.5.0",
    "ngx-bootstrap": "1.8.1",
    "osm-auth": "1.0.2",
    "osmtogeojson": "3.0.0-beta.2",
    "raven-js": "3.17.0",
    "reflect-metadata": "0.1.10",
    "rxjs": "5.4.2",
    "snyk": "1.37.0",
    "xmlbuilder": "9.0.1",
    "zone.js": "0.8.16"
  },
  "devDependencies": {
    "@angular/cli": "1.2.6",
    "@angular/compiler-cli": "4.3.2",
    "@types/jasmine": "2.5.53",
    "@types/node": "8.0.17",
    "concurrently": "3.5.0",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.28.4",
    "extract-text-webpack-plugin": "2.1.2",
    "file-loader": "0.11.2",
    "html-loader": "0.4.5",
    "html-webpack-plugin": "2.29.0",
    "jasmine": "2.6.0",
    "karma": "1.7.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-jasmine": "1.1.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.3",
    "less": "2.7.2",
    "less-loader": "4.0.4",
    "optimize-css-assets-webpack-plugin": "1.3.2",
    "optimize-js-plugin": "0.0.4",
    "script-ext-html-webpack-plugin": "1.8.5",
    "style-loader": "0.18.2",
    "to-string-loader": "1.1.5",
    "ts-loader": "2.3.2",
    "tslint": "5.4.3",
    "tslint-eslint-rules": "4.1.1",
    "tslint-loader": "3.5.3",
    "tsutils": "2.8.0",
    "typescript": "2.4.2",
    "url-loader": "0.5.9",
    "webpack": "2.7.0",
    "webpack-cleanup-plugin": "0.5.1",
    "webpack-dev-server": "2.6.1"
  },
  "snyk": true
}
