  "bugs": {
    "url": "https://github.com/dkocich/osm-pt-ngx-leaflet/issues"
  },
  "scripts": {
    "build-dev": "webpack --config webpack/webpack.config.dev.js --progress --colors",
    "build-prod": "webpack -p --config webpack/webpack.config.prod.js --colors",
    "dev": "webpack-dev-server --config webpack/webpack.config.dev.js  --content-base public/ --progress --colors",
    "lint": "tslint -c tslint.json public_src/**/*.ts",
    "postinstall": "npm run build-prod",
    "prepublish": "snyk protect",
    "start": "webpack-dev-server --content-base public/",
    "test": "snyk test && NODE_ENV=development karma start",
    "watch": "webpack --config webpack/webpack.config.dev.js --watch"
  },
  "homepage": "https://github.com/dkocich/osm-pt-ngx-leaflet#readme",
