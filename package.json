{
  "name": "osm-pt-ngx-leaflet",
  "version": "0.0.1",
  "engines": {
    "node": ">=8.1.4",
    "npm": ">=5.3.0",
    "yarn": ">=1.2.1"
  },
  "description": "An OSM public transport editor based on Leaflet and Angular JS framework.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dkocich/osm-pt-ngx-leaflet.git"
  },
  "keywords": [
    "angular",
    "leaflet",
    "openstreetmap"
  ],
  "author": "David Kocich <dkocich@gmail.com>",
